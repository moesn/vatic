{
  "$schema": "../../node_modules/vatic/schema.json",
  "table": {
    "api": "/monitor/logininfor/list",
    "title": "系统日志",
    "keyField": "id",
    "nameField": "operName",
    "export": true,
    "columns": [
      {
        "field": "seq",
        "type": "seq",
        "width": 50
      },
      {
        "title": "账号",
        "field": "account"
      },
      {
        "title": "姓名",
        "field": "todo"
      },
      {
        "title": "操作时间",
        "field": "loginTime",
        "format": "datetime"
      },
      {
        "title": "操作IP",
        "field": "ipaddr"
      },
      {
        "title": "操作内容",
        "field": "todo"
      },
      {
        "title": "操作结果",
        "field": "status",
        "width": 80,
        "type": "Tag",
        "options": [
          {
            "label": "成功",
            "value": "0",
            "color": "success"
          },
          {
            "label": "失败",
            "value": "1",
            "color": "error"
          }
        ]
      }
    ],
    "search": [
      {
        "title": "用户账号",
        "field": "todo",
        "type": "Input"
      },
      {
        "title": "操作时间",
        "field": "todo",
        "type": "RangePicker",
        "rangeFields": [
          "startTime",
          "endTime"
        ]
      }
    ]
  }
}
