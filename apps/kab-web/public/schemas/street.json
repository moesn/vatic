{
  "$schema": "../../node_modules/@vatic/smart/schema.json",
  "table": {
    "api": "/kab/region/lazy-list",
    "title": "街道",
    "keyField": "id",
    "nameField": "name",
    "export": true,
    "columns": [
      {
        "type": "seq",
        "width": 50
      },
      {
        "title": "名称",
        "field": "name",
        "align": "left",
        "treeNode": true
      }
    ],
    "search": [
      {
        "title": "关键字",
        "field": "keyword",
        "type": "Input"
      }
    ],
    "delete": "/kab/device/delDevice"
  },
  "form": {
    "title": "新增街道&修改街道",
    "detail": "/kab/device/detail/$id",
    "create": "/kab/device/addDevice",
    "update": "/kab/device/editDevice",
    "mergeFields": [
      {
        "newField": "coordinate",
        "rawFields": [
          "longitude",
          "latitude"
        ]
      }
    ],
    "items": [
    ]
  }
}
