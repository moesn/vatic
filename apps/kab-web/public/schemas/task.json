{
  "$schema": "../../node_modules/@vatic/smart/schema.json",
  "table": {
    "api": "/kab/task/pageQueryList",
    "title": "任务处理列表",
    "keyField": "id",
    "nameField": "name",
    "columns": [
      {
        "type": "seq",
        "width": 50
      },
      {
        "title": "任务名称",
        "field": "name"
      },
      {
        "title": "任务类型",
        "field": "taskTypeName"
      },
      {
        "title": "任务生成时间",
        "field": "createTime"
      },
      {
        "title": "任务处理时间",
        "field": "processTime"
      },
      {
        "title": "任务结束时间",
        "field": "endTime"
      },
      {
        "title": "任务处理结果",
        "field": "result"
      },
      {
        "title": "处理人",
        "field": "handler"
      },
      {
        "title": "任务状态",
        "field": "statusName"
      },
      {
        "title": "任务详情",
        "field": "detail"
      }
    ],
    "search": [
      {
        "title": "任务名称",
        "field": "name",
        "type": "Input"
      },
      {
        "title": "任务类型",
        "field": "eventType",
        "type": "ApiSelect",
        "api": "/kab/dict/dictDataQueryList?dictTypeId=15",
        "valueField": "code",
        "span": 1
      },
      {
        "title": "任务状态",
        "field": "status",
        "type": "ApiSelect",
        "api": "/kab/dict/dictDataQueryList?dictTypeId=14",
        "valueField": "code",
        "span": 1
      }
    ]
  },
  "operations": [
    {
      "title": "详情"
    },
    {
      "title": "进度",
      "type": "Timeline"
    },
    {
      "title": "复核",
      "form": {
        "title": "任务复核",
        "update": "/kab/role/editRole",
        "items": [
          {
            "title": "复核意见",
            "field": "todo",
            "required": true
          },
          {
            "title": "复核意见说明",
            "field": "todo",
            "type": "Textarea"
          }
        ]
      }
    }
  ]
}
